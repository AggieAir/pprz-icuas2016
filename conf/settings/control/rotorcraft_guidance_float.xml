<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="Horiz Loop">
      <dl_setting var="guidance_h.gains_f.p" min="0" step="0.01" max="10" module="guidance/guidance_h" shortname="kp_f" param="GUIDANCE_H_PGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.d" min="0" step="0.01" max="10" module="guidance/guidance_h" shortname="kd_f" param="GUIDANCE_H_DGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.i" min="0" step="0.01" max="10" module="guidance/guidance_h" shortname="ki_f" handler="set_igain_f" param="GUIDANCE_H_IGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.v" min="0" step="0.01" max="10" module="guidance/guidance_h" shortname="kv_f" param="GUIDANCE_H_VGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.a" min="0" step="0.01" max="10" module="guidance/guidance_h" shortname="ka_f" param="GUIDANCE_H_AGAIN" type="float" persistent="true"/>
    </dl_settings>

  </dl_settings>
</settings>
